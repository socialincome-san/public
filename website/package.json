    "@types/node": "^22.7.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/vimeo__player": "^2.18.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.4.47",
