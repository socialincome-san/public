{
	"name": "firestore-to-postgres",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"env:local": "dotenv -e .env.local --",
		"seed:defaults": "npm run env:local tsx scripts/seed-defaults.ts",
		"migrate:all": "npm run seed:defaults && npm run migrate:contributors && npm run migrate:local-partners && npm run migrate:recipients && npm run migrate:campaigns && npm run migrate:contributions && npm run migrate:payouts && npm run migrate:exchange-rates && npm run migrate:donation-certificates && npm run migrate:expenses && npm run migrate:surveys",
		"migrate:contributors": "npm run env:local tsx scripts/migrate-contributors.ts",
		"migrate:local-partners": "npm run env:local tsx scripts/migrate-local-partners.ts",
		"migrate:recipients": "npm run env:local tsx scripts/migrate-recipients.ts",
		"migrate:campaigns": "npm run env:local tsx scripts/migrate-campaigns.ts",
		"migrate:contributions": "npm run env:local tsx scripts/migrate-contributions.ts",
		"migrate:payouts": "npm run env:local tsx scripts/migrate-payouts.ts",
		"migrate:exchange-rates": "npm run env:local tsx scripts/migrate-exchange-rates.ts",
		"migrate:donation-certificates": "npm run env:local tsx scripts/migrate-donation-certificates.ts",
		"migrate:expenses": "npm run env:local tsx scripts/migrate-expenses.ts",
		"migrate:surveys": "npm run env:local tsx scripts/migrate-surveys.ts",
		"migrate:reset": "npm --workspace=@socialincome/website run db:migrate:reset",
		"typecheck": "tsc --noEmit"
	}
}
